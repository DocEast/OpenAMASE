<?xml version="1.0" ?>

<WindowService>
  <Window>
    <!--Shows the menu bar.-->
    <showMenuBar>true</showMenuBar>
    <!--Shows a toolbar.-->
    <showToolBar>false</showToolBar>
    <!--Sets the orientation of the toolbar.  [North, West, East]-->
    <toolbarPlacement>North</toolbarPlacement>
    <!--Shows a status bar at the bottom of the window.-->
    <showStatusBar>true</showStatusBar>
    <!--The baseline title that is shown in the title bar.-->
    <baseTitle>AMASE Playback Tool</baseTitle>
    <!--Preferred Width of window.-->
    <width>1088</width>
    <!--Preferred Height of window.-->
    <height>703</height>
    <!--An Icon to show.  Specify the path using java classpath URL conventions.-->
    <icon>/resources/amase_playback.png</icon>
    <!--x coordinate of the top left corner of the window.-->
    <x>120</x>
    <!--y coordinate of the top left corner of the window.-->
    <y>69</y>
    <!--If true, the window fills the screen-->
    <fullscreen>false</fullscreen>
  </Window>
  <!--Automatically generated layout information.  Do not edit by hand.-->
  <Layout>
    <PerspectiveModel currentPerspectiveId="default" defaultPerspectiveId="default">
      <Perspective id="default" name="default">
        <Layout>
          <DockingPortNode>
            <SplitNode dockingRegion="south" orientation="vertical" percentage="1.0" region="left">
              <DockingPortNode>
                <SplitNode dockingRegion="south" orientation="vertical" percentage="0.86026937" region="left">
                  <DockingPortNode>
                    <SplitNode dockingRegion="west" orientation="horizontal" percentage="0.60247856" region="left">
                      <DockingPortNode>
                        <DockableNode dockableId="avtas.amase.map.MapPlugin"></DockableNode>
                      </DockingPortNode>
                      <DockingPortNode>
                        <DockableNode dockableId="avtas.amase.ui.VehicleStateDisplay"></DockableNode>
                      </DockingPortNode>
                    </SplitNode>
                  </DockingPortNode>
                  <DockingPortNode>
                    <DockableNode dockableId="avtas.amase.ui.SimControls"></DockableNode>
                  </DockingPortNode>
                </SplitNode>
              </DockingPortNode>
              <DockingPortNode></DockingPortNode>
            </SplitNode>
          </DockingPortNode>
        </Layout>
      </Perspective>
    </PerspectiveModel>
  </Layout>
</WindowService>
